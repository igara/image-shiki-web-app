{
  "name": "nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist .cache && rm -rf ../rust/www/src/assets/*.js ../rust/www/src/assets/*.json ../rust/www/src/assets/*.css",
    "dev_create_parcel_image_shiki_json": "node create_parcel_json.js mithril/image_shiki/index.html dev ../rust/www/src/assets",
    "prod_create_parcel_image_shiki_json": "node create_parcel_json.js mithril/image_shiki/index.html prod ../rust/www/src/assets",
    "dev_build": "npm run clean && npm run dev_parsel_build",
    "prod_build": "npm run clean && npm run prod_parsel_build",
    "dev_parsel_build": "npm run dev_parsel_image_shiki && npm run dev_create_parcel_image_shiki_json",
    "prod_parsel_build": "npm run prod_parsel_image_shiki && npm run prod_create_parcel_image_shiki_json",
    "dev_start": "parcel mithril/image_shiki/index.html -d dist/dev/image_shiki",
    "dev_parsel_image_shiki": "parcel build mithril/image_shiki/index.html -d dist/dev/image_shiki --no-minify",
    "prod_parsel_image_shiki": "parcel build mithril/image_shiki/index.html -d dist/prod/image_shiki"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "mithril": "^1.1.6"
  },
  "devDependencies": {
    "@types/mithril": "^1.1.12",
    "autoprefixer": "^7.2.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.13.1",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.1.0",
    "postcss-modules": "^1.1.0"
  }
}
